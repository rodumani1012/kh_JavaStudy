package exception.ex;

public class Exception1_1 {
	
	public static void main(String[] args) {
		
		// 예외 발생
//		try {
//			throw new Exception();
//		} catch (Exception e) {
//			e.printStackTrace();
//		}
		
		// 1. NullPointException
		String str = null;
		
		for (int i = 0; i < str.length(); i++) {
			System.out.println(str.charAt(i));
		}
		
	}
}
